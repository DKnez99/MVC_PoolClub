@model AppUser
@{
    ViewData["Title"]= "User Details";
}

<div class="card ">
    <h4 class="card-header">@ViewData["Title"]</h4>
    <div class="card-body">
        <div class="card-text">
            Id: @Model.Id
            <hr />
            Name: @Model.FirstName @Model.LastName
            <hr />
            Email: @Model.Email
            <hr />
            Phone: @Model.PhoneNumber
        </div>
    </div>
</div>
<a asp-controller="Users" asp-action="Index" class="btn btn-primary" style="margin-top:10px">Back</a>
